package com.capgemini.gameoflife;

public class Board {
	private int width;
	private int height;
	private Cell[][] boardOfCells;

	public Board(int height,int width){
		this.height=height;
		this.width=width;
		boardOfCells=new Cell[height][width];
		
	}
	
